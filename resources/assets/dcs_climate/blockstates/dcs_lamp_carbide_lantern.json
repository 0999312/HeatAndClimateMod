{
    "multipart": [
        {   "apply": { "model": "dcs_climate:build/dcs_lamp_carbide_main" }},
        {   "when": { "side": "north" },
            "apply": { "model": "dcs_climate:build/dcs_lamp_carbide_side", "uvlock": false }
        },
        {   "when": { "side": "east" },
            "apply": { "model": "dcs_climate:build/dcs_lamp_carbide_side", "y": 90, "uvlock": false }
        },
        {   "when": { "side": "south" },
            "apply": { "model": "dcs_climate:build/dcs_lamp_carbide_side", "y": 180, "uvlock": false }
        },
        {   "when": { "side": "west" },
            "apply": { "model": "dcs_climate:build/dcs_lamp_carbide_side", "y": 270, "uvlock": false }
        }
    ]
}